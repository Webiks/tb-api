{
	"extends": "eslint:recommended",
	"parserOptions": {
		"ecmaVersion": 2018
	},
	"env": {
		"node": true,
		"commonjs": true,
	 	"es6": true
	},
	"rules": {
		"quotes": ["error", "single"],
		"semi": ["error", "always"],
		"indent": ["error", "tab",  { "SwitchCase": 1 } ],
		"no-console": "off",
		"no-useless-escape": "off"
	}
}
