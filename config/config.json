{
	"appPort": 10010,
	"paths": {
		"staticPath": "/static",
		"imagesPath": "/images"
	},
	"remote": {
		"localDomain": "http://127.0.0.1",
		"domain": "http://tb-admin.webiks.com",
		"serverDomain": "http://tb-server.webiks.com",
		"gsDatadir": "D:/GeoServer/data_dir"
	},
	"mongodb": {
		"port": 27017,
		"name": "tb_database",
		"url": "mongodb://127.0.0.1:27017"
	},
	"login":	{
		"usernameKey": "TB_USERNAME",
		"passwordKey": "TB_PASSWORD"
	},
