	"upload": {
		"headers": {
			"authorization": "Basic YWRtaW46Z2Vvc2VydmVy",
			"Content-Type": "application/json",
			"accept": "application/json"
		},
		"maxFileSize": 50000000000,
		"defaultWorldId": "public"
	}
